@{
	import (
		m "1bwiki/model"

		"1bwiki/tmpl/layout/base"
		"1bwiki/tmpl/helper"
	)
	var u *m.User
	var pv *m.PageView
}

@section title {
	return pv.NiceTitle
}

@section user {
	@helper.User(u)
}

@raw(Nav(pv.NiceTitle, "article"))

<div class="ui orange message">
    <p>This is an old version of the page. This paragraph could use more information</p>
</div>

@raw(pv.Html())